{
  "manifest_version": 3,
  "name": "YouTube Video Logger",
  "version": "1.1",
  "description": "Logs a message when a YouTube video finishes playing.",
  "permissions": ["scripting", "activeTab"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "YouTube Video Logger",
    "action": {
      "type": "script",
      "script": "content.js"
    }
  }
}